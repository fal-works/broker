package broker.entity;

/**
	Basic entity that has position and velocity.
**/
class BasicEntity {
	/**
		X-component of the position.
	**/
	var x: Float = 0;

	/**
		Y-component of the position.
	**/
	var y: Float = 0;

	/**
		X-component of the velocity.
	**/
	var vx: Float = 0;

	/**
		Y-component of the velocity.
	**/
	var vy: Float = 0;
}
